    "morgan": "^1.10.0",
    "next": "13.1.6",
    "next-api-decorators": "^2.0.1",
    "path-to-regexp": "^6.2.1",
    "picocolors": "^1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.20.6"
