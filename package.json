    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5",
    "typescript-plugin-css-modules": "^4.1.1",
    "vitest": "^0.28.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "@babel/plugin-proposal-decorators",
